{
  "name": "kes-bingo",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build:1-next": "tsc --noEmit && cross-env NODE_ENV=production next build",
    "build:2-server": "tsc --project tsconfig.server.json",
    "build": "rm -rf ./dist && run-s build:*",
    "check": "next lint && tsc --noEmit",
    "dev:wss": "cross-env PORT=3001 tsx watch src/server/wssDevServer.ts --tsconfig tsconfig.server.json ",
    "dev:next": "next dev",
    "dev": "run-p dev:*",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "format:write": "prettier --write \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "preview": "next build && next start",
    "start:next": "next start",
    "start:server": "cross-env PORT=3001 tsx watch src/server/wssDevServer.ts --tsconfig tsconfig.server.json",
    "start": "run-p start:*",
    "temp": "cross-env NODE_ENV=production tsx ./src/server/wssProdServer.ts",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@auth/unstorage-adapter": "^2.10.0",
    "@next/env": "^15.4.6",
    "@tanstack/react-query": "^5.69.0",
    "@trpc/client": "^11.0.0",
    "@trpc/react-query": "^11.0.0",
    "@trpc/server": "^11.0.0",
    "@uploadthing/react": "^7.3.2",
    "clsx": "^2.1.1",
    "lucide-react": "^0.539.0",
    "next": "^15.2.3",
    "next-auth": "^5.0.0-beta.29",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "server-only": "^0.0.1",
    "superjson": "^2.2.1",
    "tailwind-merge": "^3.3.1",
    "tsx": "^4.20.4",
    "unstorage": "^1.16.1",
    "uploadthing": "^7.7.3",
    "ws": "^8.18.3",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@tailwindcss/postcss": "^4.0.15",
    "@types/node": "^22.17.1",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@types/ws": "^8.18.1",
    "cross-env": "^10.0.0",
    "eslint": "^9.23.0",
    "eslint-config-next": "^15.2.3",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.5.3",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "tailwindcss": "^4.0.15",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.27.0"
  },
  "ct3aMetadata": {
    "initVersion": "7.39.3"
  },
  "packageManager": "npm@10.8.1"
}
